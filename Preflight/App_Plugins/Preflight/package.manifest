{
  "contentApps": [
    {
      "name": "Preflight", // required - the name that appears under the icon, everyone loves cake, right?
      "alias": "preflight", // required - unique alias for your app
      "weight": 0, // optional, default is 0, use values between -99 and +99 to appear between the existing Content (-100) and Info (100) apps
      "icon": "icon-paper-plane", // required - the icon to use
      "view": "~/app_plugins/preflight/backoffice/views/contentapp.html" // required - the location of the view file
    }
  ],
  "javascript": [
    "~/App_Plugins/preflight/backoffice/resources/preflight.service.js",
    "~/App_Plugins/preflight/backoffice/controllers/preflight.dialog.controller.js",
    "~/App_Plugins/preflight/backoffice/controllers/preflight.dashboard.controller.js",
    "~/App_Plugins/preflight/backoffice/controllers/preflight.help.overlay.controller.js",
    "~/App_Plugins/preflight/backoffice/interceptors/preflight.save.interceptor.js",
    "~/App_Plugins/preflight/backoffice/directives/preflight.card.directive.js",
    "~/App_Plugins/preflight/backoffice/directives/preflight.stateicon.directive.js",
    "~/App_Plugins/preflight/backoffice/directives/preflight.resultintro.directive.js"

  ],
  "css": [
    "~/App_Plugins/preflight/backoffice/css/styles.css"
  ]
}